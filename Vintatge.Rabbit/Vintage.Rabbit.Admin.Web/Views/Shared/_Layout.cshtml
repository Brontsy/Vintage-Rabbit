<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,400italic,700,800' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:300,200,100' rel='stylesheet' type='text/css'>

    <!-- Bootstrap core CSS -->
    <link href="/content/js/bootstrap/dist/css/bootstrap.css" rel="stylesheet">
    <link rel="stylesheet" href="/content/fonts/font-awesome-4/css/font-awesome.min.css">
    <link href="/content/js/jquery.icheck/skins/square/blue.css" rel="stylesheet">
    <link href="/content/css/style.less" rel="stylesheet">


    <script src="/content/js/jquery.js"></script>
    <script type="text/javascript" src="/content/js/jquery.nanoscroller/jquery.nanoscroller.js"></script>
    <script type="text/javascript" src="/content/js/jquery.sparkline/jquery.sparkline.min.js"></script>
    <script type="text/javascript" src="/content/js/jquery.easypiechart/jquery.easy-pie-chart.js"></script>
    <script type="text/javascript" src="/content/js/behaviour/general.js"></script>
    <script src="/content/js/jquery.ui/jquery-ui.js" type="text/javascript"></script>
    <script type="text/javascript" src="/content/js/jquery.nestable/jquery.nestable.js"></script>
    <script type="text/javascript" src="/content/js/bootstrap.switch/bootstrap-switch.min.js"></script>
    <script type="text/javascript" src="/content/js/bootstrap.datetimepicker/js/bootstrap-datetimepicker.min.js"></script>
    <script src="/content/js/jquery.select2/select2.min.js" type="text/javascript"></script>
    <script src="/content/js/bootstrap.slider/js/bootstrap-slider.js" type="text/javascript"></script>
    <script type="text/javascript" src="/content/js/jquery.gritter/js/jquery.gritter.js"></script>
    <script type="text/javascript" src="/content/js/jquery.icheck/icheck.min.js"></script>
    <script src="/content/js/bootstrap/dist/js/bootstrap.min.js"></script>


@*@Styles.Render("~css")
@Scripts.Render("~/bundles/modernizr")*@

</head>
<body>

    <div id="head-nav" class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="fa fa-gear"></span>
                </button>
                <a class="navbar-brand" href="#"><span>Vintage Rabbit</span></a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right user-nav">
                    <li class="dropdown profile_menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i>Luke Bronts<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">My Account</a></li>
                            <li><a href="#">Profile</a></li>
                            <li><a href="#">Messages</a></li>
                            <li class="divider"></li>
                            <li>@Html.RouteLink("Logout", Routes.Membership.Logout)</li>
                        </ul>
                    </li>
                </ul>
   
            </div><!--/.nav-collapse -->
        </div>
    </div>

   

    <div id="cl-wrapper">

        <div class="cl-sidebar">
            <!-- Sidebar Menu -->
            <div class="cl-navblock">
                <ul class="cl-vnavigation">
                    <li>
                        <a href="/"><i class="fa fa-home"></i>Dashboard</a>
                    </li>
                    <li>
                        <a href="@Url.RouteUrl(Routes.Products.Buy)"><i class="fa fa-dollar"></i>Buy</a>
                    </li>
                    <li>
                        <a href="@Url.RouteUrl(Routes.Products.Hire)"><i class="fa fa-calendar"></i>Hire</a>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-picture-o"></i>Themes</a>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-pencil-square-o"></i>Orders</a>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-users"></i>Members</a>
                    </li>
                </ul>
            </div>
        </div>

        <div class="container-fluid" id="pcont">
                @RenderBody();
        </div>

    </div>
    @*@RenderSection("scripts", required: false)*@

    <script type="text/javascript">
        $(document).ready(function () {
            //initialize the javascript
            App.init();
            $('input').iCheck({
                checkboxClass: 'icheckbox_square-blue checkbox',
                radioClass: 'iradio_square-blue'
            });

            $("#check-all").on('ifChanged', function () {
                var checkboxes = $(".items").find(':checkbox');
                if ($(this).is(':checked')) {
                    checkboxes.iCheck('check');
                } else {
                    checkboxes.iCheck('uncheck');
                }
            });
        });
    </script>

</body>
</html>
