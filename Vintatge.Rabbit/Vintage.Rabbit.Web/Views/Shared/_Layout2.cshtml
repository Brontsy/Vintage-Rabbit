<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="/Content/less/v@(ViewBag.Version)/site.less" type="text/css" charset="utf-8" />
    <link href='http://fonts.googleapis.com/css?family=PT+Sans:400,700' rel='stylesheet' type='text/css'>

</head>
<body>

    
    <div class="container-fluid">
    
        <div class="gutter header">
            <div class="row-fluid">
                <div class="span3">
                    <h1>Vintage Ra<span>bb</span>it</h1>
                </div>
            
                <div class="span4">
                    <div class="search">
				        <div class="button-search">
                            <i class="fa fa-search"></i>
				        </div>
				        <input type="text" name="search" placeholder="Search" value="">
			        </div>
                </div>
                
            <div class="span3">
                <div class="row-fluid">
                    <div class="span8">
                        <div class="shopping-cart">
                            <div class="cart-container text-small">
                                Shopping Cart - 0 Items
                                <i class="fa fa-arrow-circle-down right"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="span2">
                <img src="https://member.carsalesnetwork.com.au/Content/seller/images/myadverts/icon-facebook.png" />
                <img src="https://member.carsalesnetwork.com.au/Content/seller/images/myadverts/icon-twitter.png" />
                <img src="https://member.carsalesnetwork.com.au/Content/seller/images/myadverts/icon-google-plus.png" />
                </div>
            </div>
            </div>
        </div>

        <div class="nav">
            <div class="gutter">
                <ol class="row-fluid">
                    <li class="span2 offset1">
                        @Html.RouteLink("Buy", Routes.Buy)
                    </li>
                    <li class="span2">
                        @Html.RouteLink("Hire", Routes.Hire)
                    </li>
                    <li class="span2">
                        @Html.RouteLink("Styles", Routes.Styles)
                    </li>
                    <li class="span2">
                        @Html.RouteLink("Blog", Routes.Blog)
                    </li>
                    <li class="span2">
                        @Html.RouteLink("Contact us", Routes.ContactUs)
                    </li>
                </ol>
            </div>
        </div>

        
        
        <div class="gutter">
            <div class="row-fluid">
            
                <div class="span12">
                    <div class="image-container">
                        <img src="/content/images/teddy-bear.jpg" />
                        @Html.RouteLink("Styles", Routes.Styles)

                        <div class="hidden">
                            Styles: We have 6 different party themes to choose from.
                            Let us look after the styling of your childs birthday!

                            Hire: Need something extra?
                            We have lots of items for hire, ranging cups through to large tables.

                            Buy: Games, Party Supplies and Gifts
                            We have 100's of products for you to choose from.
                        </div>

                        <div class="nav-controls">
                            <div class="left">
                                <i class="fa fa-angle-left"></i>
                            </div>
                            <div class="right">
                                <i class="fa fa-angle-right"></i>
                            </div>
                        </div>
                    </div>
                </div>
            
            </div>
        </div>
        
        <div class="gutter">
            <div class="row-fluid">
                <div class="span4">
                    <div class="highlight">
                        <div class="row-fluid">
                            <i class="span2 fa fa-truck"></i>
                            <h3 class="span10">
                                Free delivery over $100
                                <em>Australia wide</em>
                            </h3>
                        </div>
                    </div>
                </div>
                <div class="span4">
                    <div class="highlight">
                        <div class="row-fluid">
                            <i class="fa fa-question-circle"></i>
                            <h3 class="span10">
                                Have a question?
                                <em>Give us a call any time on 13 41 41</em>
                            </h3>
                        </div>
                    </div>
                </div>
                <div class="span4">
                    <div class="highlight">
                        <div class="row-fluid">
                            <i class="fa fa-question-circle"></i>
                            <h3 class="span10">
                                Another promotion
                                <em>Extra text related to the promotion</em>
                            </h3>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row-fluid hidden">
                <div class="span8">
                    <div class="highlight">
                        <div class="row-fluid">
                            <i class="fa fa-rocket"></i>
                            <h3 class="span10">
                                Checkout our new Rocket Party Style
                            </h3>
                        </div>
                        <a href="/rockets">Click here to view our latest party style.</a>
                        <img class="right" src="/content/images/Soccer-dessert-table.jpg"  height="100"/>
                    </div>
                </div>
            </div>
        </div>
        
        <div class="product-list">
            <h2>Featured</h2>
            <div class="gutter">
            <div class="row-fluid">
                <div class="span2">
                    <div class="item-container">
                        <div class="item">
                            <img src="/content/images/rainbow-cups---hr.jpg" width="100%"/>
                            <div class="name">
                                <div class="start"></div>
                                <h4>Rainbow Stripe Paper Cups</h4>
                                <div class="end"></div>
                            </div>
                            <div class="price">$6.95</div>
                            <div class="product-controls text-small">
                                <div class="left">
                                    <i class="fa fa-shopping-cart"></i>
                                    Add to cart
                                </div>
                                <div class="right">
                                    <i class="fa fa-search"></i>
                                    Preview
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="span2">
                    <div class="item-container">
                        <div class="item">
                            <img src="/content/images/age-05-102-spot.jpg" width="100%"/>
                            <div class="name">
                                <div class="start"></div>
                                <h4>Tall Milk Bottle</h4>
                                <div class="end"></div>
                            </div>
                            <div class="price">$9.95</div>
                            <div class="product-controls text-small">
                                <div class="left">
                                    <i class="fa fa-shopping-cart"></i>
                                    Add to cart
                                </div>
                                <div class="right">
                                    <i class="fa fa-search"></i>
                                    Preview
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="span2">
                    <div class="item-container">
                        <div class="item">
                            <img src="/content/images/lollies_1.jpg" width="100%"/>
                            <div class="name">
                                <div class="start"></div>
                                <h4>Bottle of Lollies</h4>
                                <div class="end"></div>
                            </div>
                            <div class="price">$7.95</div>
                            <div class="product-controls text-small">
                                <div class="left">
                                    <i class="fa fa-shopping-cart"></i>
                                    Add to cart
                                </div>
                                <div class="right">
                                    <i class="fa fa-search"></i>
                                    Preview
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="span2">
                    <div class="item-container">
                        <div class="item">
                            <img src="/content/images/sambellina_rainbow_treat_box__23750.1377144079.1280.1280.jpg" width="100%"/>
                            <div class="name">
                                <div class="start"></div>
                                <h4>Rainbow Stripe Paper Cups</h4>
                                <div class="end"></div>
                            </div>
                            <div class="price">$4.95</div>
                            <div class="product-controls text-small">
                                <div class="left">
                                    <i class="fa fa-shopping-cart"></i>
                                    Add to cart
                                </div>
                                <div class="right">
                                    <i class="fa fa-search"></i>
                                    Preview
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="span2">
                    <div class="item-container">
                        <div class="item">
                            <img src="/content/images/confetti-party-in-a-box.jpg" width="100%"/>
                            <div class="name">
                                <div class="start"></div>
                                <h4 title="Confetti Party in a Box">Confetti Party in a Box</h4>
                                <div class="end"></div>
                            </div>
                            <div class="price">$14.95</div>
                            <div class="product-controls text-small">
                                <div class="left">
                                    <i class="fa fa-shopping-cart"></i>
                                    Add to cart
                                </div>
                                <div class="right">
                                    <i class="fa fa-search"></i>
                                    Preview
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="span2">
                    <div class="item-container">
                        <div class="item">
                            <img src="/content/images/il_570xn.445330436_5xg8.jpg" width="100%"/>
                            <div class="name">
                                <div class="start"></div>
                                <h4>Party Confetti</h4>
                                <div class="end"></div>
                            </div>
                            <div class="price">$8.00</div>

                            <div class="product-controls text-small">
                                <div class="left">
                                    <i class="fa fa-shopping-cart"></i>&nbsp;
                                    Add to cart
                                </div>
                                <div class="right">
                                    <i class="fa fa-search"></i>
                                    Preview
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            </div>
            <div class="row-fluid">
                <div class="span1 offset11">
                    <div class="nav-controls">
                        <div class="left">
                            <i class="fa fa-angle-left"></i>
                        </div>
                        <div class="right">
                            <i class="fa fa-angle-right"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        @RenderBody()

    </div>

    <!-- scripts -->


</body>
</html>
